{
  "name": "ably-livesync-neon",
  "scripts": {
    "dev": "prisma generate && next dev --port 8989",
    "build": "prisma generate && next build",
    "start": "next start",
    "schema": "tsx schema.tsx",
    "db": "npm run schema && prisma generate && prisma db push && npx prisma db execute --file ./prisma/migrations/trigger.sql --schema prisma/schema.prisma && prisma db seed",
    "fmt": "prettier --write '**/*' --ignore-unknown",
    "lint": "next lint",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@ably-labs/models": "^0.0.4",
    "@heroicons/react": "^2.0.18",
    "@neondatabase/serverless": "^0.10.4",
    "@prisma/adapter-neon": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@radix-ui/themes": "^3.2.1",
    "@stackframe/stack": "^2.8.54",
    "@stackframe/stack-sc": "^2.8.54",
    "ably": "^2.14.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "next": "^16.0.6",
    "prettier": "^3.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-feather": "^2.0.10",
    "uuid": "^13.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.8.1",
    "@types/lodash": "^4.17.13",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.5.13",
    "autoprefixer": "^10.4.22",
    "dotenv": "^17.2.3",
    "postcss": "^8.5.6",
    "prisma": "^7.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3"
  }
}
